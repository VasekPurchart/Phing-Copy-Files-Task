{
	"name": "vasek-purchart/phing-copy-files-task",
	"description": "Copy files with better control over copying than the default Phing CopyTask",
	"keywords": ["phing", "task", "copy", "file"],
	"license": "MIT",
	"authors": [
		{
			"name": "Va≈°ek Purchart",
			"email": "me@vasekpurchart.cz",
			"homepage": "http://vasekpurchart.cz"
		}
	],
	"require": {
		"php": "~7.0"
	},
	"require-dev": {
		"consistence/coding-standard": "0.13",
		"jakub-onderka/php-console-highlighter": "0.3.2",
		"jakub-onderka/php-parallel-lint": "0.9.2",
		"phing/phing": "2.16",
		"phpunit/phpunit": "6.0.10"
	},
	"autoload": {
		"psr-4": {
			"VasekPurchart\\Phing\\CopyFiles\\": [
				"src"
			]
		},
		"classmap": [
			"src"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"VasekPurchart\\Phing\\CopyFiles\\": [
				"tests"
			]
		},
		"classmap": [
			"tests"
		]
	},
	"config": {
		"bin-dir": "bin"
	}
}
